    "minisearch": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "prism-svelte": "^0.4.7",
    "prismjs": "^1.28.0",
    "remark-slug": "^6.0.0",
    "svelte": "^3",
