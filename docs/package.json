    "build:routify": "routify run -b",
    "build:svite": "vite build"
  },
  "devDependencies": {
    "@sveltech/routify": "^1.9.10",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "carbon-components-svelte": "../",
    "carbon-icons-svelte": "^12.10.0",
    "clipboard-copy": "^4.0.1",
    "cross-env": "^7.0.3",
    "mdsvex": "^0.12.3",
    "minisearch": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "prism-svelte": "^0.4.7",
    "prismjs": "^1.29.0",
    "remark-slug": "^6.0.0",
    "svelte": "^4.2.18",
    "vite": "^5.4.0"
  },
  "routify": {
    "routifyDir": ".routify",
    "dynamicImports": true,
    "extensions": [
      "svelte",
      "svx"
